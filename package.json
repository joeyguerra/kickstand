{
  "name": "kickstand",
  "version": "1.1.0",
  "description": "Static site generator with Express + Handlebars (and Markdown)",
  "main": "index.js",
  "directories": {
    "lib": "lib",
    "test": "tests"
  },
  "dependencies": {
    "express": "^4.17.1",
    "handlebars": "^4.3.0",
    "markdown-it": "^8.4.2",
    "markdown-it-meta": "^0.0.1",
    "mkdirp": "^0.5.1"
  },
  "devDependencies": {
    "mocha": "^5.2.0",
    "nodemon": "^1.19.1"
  },
  "scripts": {
    "test": "mocha tests/**/*.js",
    "test:dev": "mocha tests/**/*.js --watch",
    "start": "node server.js",
    "dev": "nodemon server.js --watch ./templates -e js,html,md,css server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joeyguerra/kickstand.git"
  },
  "keywords": [
    "static",
    "generator",
    "javascript",
    "express",
    "handlebars",
    "markdown"
  ],
  "author": "Joey Guerra",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joeyguerra/kickstand/issues"
  },
  "homepage": "https://github.com/joeyguerra/kickstand#readme"
}
